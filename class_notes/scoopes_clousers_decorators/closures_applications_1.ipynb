    "def averager():\n",
    "    self.total = 0\n",
    "    self.count = 0\n",
    "\n",
    "    def add(self,number):\n",
    "        self.total += number\n",
    "        self.count += 1\n",
    "        total = total + number\n",
    "        count = count + 1\n",
    "        return self.total / self.count\n",
    "    return add"
   ]
